{
  "name": "mingus",
  "version": "0.0.1",
  "description": "A testing library for React.",
  "main": "index.js",
  "scripts": {
    "check_coverage": "babel-istanbul check-coverage .coverage/coverage.json",
    "cover": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- --recursive tests",
    "lint": "eslint src tests",
    "test": "npm run lint && npm run cover && npm run check_coverage"
  },
  "keywords": [
    "test",
    "unit",
    "unit test",
    "react"
  ],
  "author": "Jeff McRiffey",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.6.7",
    "chai": "^3.2.0",
    "mocha": "^2.2.5",
    "sinon": "^1.15.4"
  },
  "devDependencies": {
    "babel-istanbul": "^0.2.10",
    "babel-runtime": "^5.6.7",
    "eslint": "^1.1.0",
    "eslint-plugin-react": "^3.2.1",
    "react": "^0.13.3"
  }
}
