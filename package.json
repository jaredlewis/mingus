{
  "name": "mingus",
  "version": "0.4.0",
  "description": "A testing library for React.",
  "main": "index.js",
  "bin": {
    "mingus": "bin/mingus"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "check-coverage": "./bin/mingus check-coverage .coverage/coverage.json",
    "cover": "./bin/mingus cover",
    "lint": "eslint src test",
    "prepublish": "npm run build",
    "test": "npm run lint && npm run cover && npm run check-coverage",
    "unit": "./bin/mingus unit"
  },
  "keywords": [
    "test",
    "unit",
    "unit test",
    "react",
    "testing"
  ],
  "author": "Jeff McRiffey",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.23",
    "babel-istanbul": "^0.3.20",
    "babel-runtime": "^5.8.25",
    "chai": "^3.3.0",
    "mocha": "^2.3.3",
    "react": "^0.14.0",
    "sinon": "^1.17.1"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "eslint": "^1.5.1",
    "eslint-plugin-react": "^3.4.2"
  }
}
