{
  "name": "mingus",
  "version": "0.1.2",
  "description": "A testing library for React.",
  "main": "index.js",
  "bin": {
    "mingus": "bin/mingus"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "check-coverage": "./bin/mingus check-coverage .coverage/coverage.json",
    "cover": "./bin/mingus cover",
    "lint": "eslint src test",
    "prepublish": "npm run build",
    "test": "npm run lint && npm run cover && npm run check-coverage"
  },
  "keywords": [
    "test",
    "unit",
    "unit test",
    "react",
    "testing"
  ],
  "author": "Jeff McRiffey",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.6.7",
    "babel-istanbul": "^0.2.10",
    "babel-runtime": "^5.6.7",
    "chai": "^3.2.0",
    "mocha": "^2.2.5",
    "react": "^0.13.3",
    "sinon": "^1.15.4"
  },
  "devDependencies": {
    "eslint": "^1.1.0",
    "eslint-plugin-react": "^3.2.1"
  }
}
